<library path="lib/librls_rviz_plugins">
  <class name="rls_rviz_plugins/ForcePanel"
         type="rls_rviz_plugins::ForcePanel"
         base_class_type="rviz::Panel">
    <description>
      shows the measured normal and resistive forces.
    </description>
  </class>
  <class name="rls_rviz_plugins/FailureReport"
         type="rls_rviz_plugins::FailureReport"
         base_class_type="rviz::Panel">
    <description>
      prompts cutting anomalies
    </description>
  </class>
  <class name="rls_rviz_plugins/ParamTune"
         type="rls_rviz_plugins::ParamTune"
         base_class_type="rviz::Panel">
    <description>
      Tune parameters
    </description>
  </class>
<class name="rls_rviz_plugins/MenuPanel"
         type="rls_rviz_plugins::MenuPanel"
         base_class_type="rviz::Panel">
    <description>
      A menu that includes other functional panels
    </description>
  </class>
<class name="rls_rviz_plugins/PlotForces"
         type="rls_rviz_plugins::PlotForces"
         base_class_type="rviz::Panel">
    <description>
      Plot measured and estimated resistive force and their error 
    </description>
  </class>
<class name="rls_rviz_plugins/PlotEstParams"
         type="rls_rviz_plugins::PlotEstParams"
         base_class_type="rviz::Panel">
    <description>
      Plot estimated cutting force and coefficient of friction 
    </description>
  </class>
<class name="rls_rviz_plugins/ParamTuneViz"
         type="rls_rviz_plugins::ParamTuneViz"
         base_class_type="rviz::Panel">
    <description>
      Visualize Parameter Tuning 
    </description>
  </class>


